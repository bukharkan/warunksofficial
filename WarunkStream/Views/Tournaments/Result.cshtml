@{
    ViewBag.Title = "Home";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel='stylesheet' type='text/css' href='https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,400italic'>
    <link rel="stylesheet" type="text/css" href="~/content/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/content/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/unify.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/unify-style-switcher.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/unify-globals.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/unify-core.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/unify-components.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/styles.op-event.css">

    <title>@ViewBag.Title - Warunk Stream</title>
</head>

<body>
    <div class="main-body">
        <div class="container">
            <div class="row">
                <div class="main-page">
                    <div class="contact-content-upper box">
                        <div class="row">
                            <div class="col-sm-12 col-md-6  contact_right">
                                <div class="info">
                                    <div class="proin-title">
                                        <h3>Turnamen PUBG Mobile - AIROKES SEASON 1</h3>
                                        <h4>22 - 23 Mei 2020</h4>
                                    </div>
                                    <img src="https://s3-ap-southeast-1.amazonaws.com/ies-public-storage/images/air-rokes.jpeg" alt="Image" class="img-responsive">
                                    <p class="proin-right-text g-font-size-14">
                                        Biaya pendaftaran IDR 25.000,- / Slot
                                        Venue : Online
                                        Open 100 Slot
                                        Multi Slot On
                                        No Emulator | Mobile Only
                                        No Cheat | No Teaming
                                        Squad Mode
                                        Sistem Point
                                        Kualifikasi 3 Match : Vikendi TPP + Miramar TPP + Sanhok TPP
                                        Grand Final 3 Match : Vikendi TPP + Miramar TPP + Sanhok TPP
                                    </p>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-6 g-pa-15">
                                <div class="proin-title">
                                    <h3>Grup Chat</h3>

                                </div>
                                <iframe src="https://minnit.chat/warunksofficial?embed&transparent&nickname=LRxPublic" style="border:none;width:90%;height:500px;" allowTransparency="true"></iframe><br />
                                <p class="proin-right-text g-font-size-14" style="color:red">Grup Chat ini bertujuan untuk info room & diskusi mengenai turnamen bagi tim yang sudah terdaftar</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <footer class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 footer">
                    <p class="copyright">Copyright © 2020 Warunk Stream | Design: template mo</p>
                </div>
            </footer>  <!-- .row -->
        </div>
    </div> <!-- .main-body -->
    <!-- JavaScript -->
    <script src="~/content/js/jquery-1.11.3.min.js"></script>
    <script src="~/content/js/bootstrap.min.js"></script>

</body>
</html>

