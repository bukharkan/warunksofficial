
@{
    ViewBag.Title = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>Certificate - Warunk Stream</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--
    Coming Template
    http://www.templatemo.com/preview/templatemo_447_coming
    -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,400italic,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/Content/Certificate/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/Certificate/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/Certificate/css/animate.min.css">
    <link rel="stylesheet" href="~/Content/Certificate/css/templatemo-style.css">
</head>
<body>
    <!-- start preloader -->
    <div class="preloader">
        <div class="sk-spinner sk-spinner-three-bounce">
            <div class="sk-bounce1"></div>
            <div class="sk-bounce2"></div>
            <div class="sk-bounce3"></div>
        </div>
    </div>
    <!-- end preloader -->
    <!---start-countdown-timer-->
    <section id="countdown">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h1 class="wow bounceIn">Search<small> Certificate</small></h1>
                    <h2 class="element">
                        <span class="sub-element">Input your certificate Id to get all Profit.</span>
                        <span class="sub-element">Redeem Profit of Certificate in Here!</span>
                        <span class="sub-element">Thank you for joining our tournaments</span>
                    </h2>
                    <form action="#" method="post">
                        <div class="col-md-3 col-sm-3"></div>
                        <div class="col-md-4 col-sm-4">
                            <input type="email" class="form-control" placeholder="Your Certificate ID">
                        </div>
                        <div class="col-md-2 col-sm-2">
                            <input type="submit" class="form-control" value="SEARCH">
                        </div>
                    </form>
                    <!-- subscribe form -->
                </div>
            </div>
        </div>
    </section>
    <section class="features text-center">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="title wow bounceIn">
                        <h2>Profit of Certificate</h2>
                    </div>
                    <div class="col-md-3 col-sm-6 wow fadeInUp templatemo-feature" data-wow-delay="0.6s">
                        <i class="fa fa-ticket"></i>
                        <h4 class="text-uppercase">Original Certificate</h4>
                        <p>You can check the certificate is original or fake.</p>
                    </div>
                    <div class="col-md-3 col-sm-6 wow fadeInUp templatemo-feature" data-wow-delay="0.6s">
                        <i class="fa fa-spotify"></i>
                        <h4 class="text-uppercase">Spotify Premium Family</h4>
                        <p>Get free account of Spotify Premium Family, you can share it to all member of team.</p>
                    </div>
                    <div class="col-md-3 col-sm-6 wow fadeInUp templatemo-feature" data-wow-delay="0.6s">
                        <i class="fa fa-retweet"></i>
                        <h4 class="text-uppercase">Free Pass Next Tournament</h4>
                        <p>Your team will get free pass the next our tournament.</p>
                    </div>
                    <div class="col-md-3 col-sm-6 wow fadeInUp templatemo-feature" data-wow-delay="0.6s">
                        <i class="fa fa-shopping-cart"></i>
                        <h4 class="text-uppercase">Shopping Discount</h4>
                        <p>Get a discount price in our store.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <p class="templatemo-copyright">Copyright © 2020 Warunk Stream | Design: template mo</p>
                    <ul class="social_icon">
                        <li><a href="#" class="fa fa-facebook"></a></li>
                        <li><a href="#" class="fa fa-twitter"></a></li>
                        <li><a href="#" class="fa fa-google-plus"></a></li>
                        <li><a href="#" class="fa fa-linkedin"></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/Content/Certificate/js/jquery.js"></script>
    <script src="~/Content/Certificate/js/bootstrap.min.js"></script>
    <script src="~/Content/Certificate/js/wow.min.js"></script>
    <script src="~/Content/Certificate/js/typed.js"></script>
    <script src="~/Content/Certificate/js/custom.js"></script>
</body>
</html>

