                    <div class="col-md-6 form-group g-mb-30">
                        <input class="form-control form-control-md rounded-0" type="text" name="nick3" placeholder="Nick Player 3">
                    </div>

                    <div class="col-md-6 form-group g-mb-30">
                        <input class="form-control form-control-md rounded-0" type="text" name="id3" placeholder="Id Player 3">
                    </div>

                    <div class="col-md-6 form-group g-mb-30">
                        <input class="form-control form-control-md rounded-0" type="text" name="nick4" placeholder="Nick Player 4">
                    </div>

                    <div class="col-md-6 form-group g-mb-30">
                        <input class="form-control form-control-md rounded-0" type="text" name="id4" placeholder="Id Player 4">
                    </div>

                    <div class="form-group text-center m-t-20">
                        <div class="col-xs-12">
                            <button type="submit" class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light"> Daftar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
